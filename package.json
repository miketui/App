{
  "name": "haus-of-basquiat",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node backend/index.js",
    "backend": "nodemon backend/index.js",
    "backend:dev": "nodemon backend/index.js",
    "backend:start": "node backend/index.js",
    "migrate": "node backend/scripts/migrate.js",
    "seed": "node backend/scripts/seed.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:all": "npm run build && npm run build:backend",
    "build:backend": "cd backend && npm run build",
    "deploy": "npm run build:all && npm run migrate",
    "deploy:staging": "npm run build:all && npm run migrate:staging",
    "deploy:production": "npm run build:all && npm run migrate:production",
    "pwa:generate": "workbox generateSW workbox-config.js",
    "pwa:inject": "workbox injectManifest workbox-config.js",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.cache",
    "clean:all": "rm -rf dist node_modules package-lock.json && npm install",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.1",
    "@supabase/supabase-js": "^2.38.4",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "redis": "^4.6.10",
    "stripe": "^14.7.0",
    "multer": "^1.4.5-lts.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "react-dropzone": "^14.2.3",
    "react-hot-toast": "^2.4.1",
    "lucide-react": "^0.294.0",
    "date-fns": "^2.30.0",
    "react-query": "^3.39.3",
    "react-hook-form": "^7.48.2",
    "framer-motion": "^10.16.16",
    "react-helmet-async": "^1.3.0",
    "react-error-boundary": "^4.0.11",
    "react-infinite-scroll-component": "^6.1.0",
    "react-share": "^4.4.1",
    "react-copy-to-clipboard": "^5.1.0",
    "react-tooltip": "^5.26.3",
    "react-modal": "^3.16.1",
    "react-select": "^5.8.0",
    "react-loading-skeleton": "^3.3.1",
    "react-spinners": "^0.13.8"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "vite": "^5.0.8"
  }
}
